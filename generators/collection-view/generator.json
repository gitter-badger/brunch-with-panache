{
  "description": "Chaplin collection view and unit test",
  "files": [
    {
      "from": "collection-view.coffee.hbs",
      "to": "app/views/{{name}}/{{name}}-collection-view.coffee"
    },
    {
      "from": "template.hbs.hbs",
      "to": "app/views/{{name}}/templates/{{name}}-collection.hbs"
    },
    {
      "from": "item/item-view.coffee.hbs",
      "to": "app/views/{{name}}/{{name}}-item-view.coffee"
    },
    {
      "from": "item/template.hbs.hbs",
      "to": "app/views/{{name}}/templates/{{name}}-item.hbs"
    },
    {
      "from": "test.coffee.hbs",
      "to": "test/code/views/{{name}}-test.coffee"
    }
  ],
  "dependencies": []
}
