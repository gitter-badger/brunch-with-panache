{
  "name": "view",
  "description": "Chaplin view and unit test",
  "files": [
    {
      "from": "view.coffee.hbs",
      "to": "app/views/{{name}}/{{name}}-view.coffee"
    },
    {
      "from": "template.hbs.hbs",
      "to": "app/views/{{name}}/templates/{{name}}.hbs"
    },
    {
      "from": "test.coffee.hbs",
      "to": "test/code/views/{{name}}-test.coffee"
    }
  ],
  "dependencies": []
}
