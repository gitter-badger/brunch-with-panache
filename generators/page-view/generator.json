{
  "name": "page-view",
  "description": "Chaplin page type view and unit test",
  "files": [
    {
      "from": "view.coffee.hbs",
      "to": "app/views/{{name}}/{{name}}-page-view.coffee"
    },
    {
      "from": "template.hbs.hbs",
      "to": "app/views/{{name}}/templates/{{name}}-page.hbs"
    },
    {
      "from": "test.coffee.hbs",
      "to": "test/code/views/{{name}}-page-test.coffee"
    }
  ],
  "dependencies": []
}
